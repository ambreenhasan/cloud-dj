
   <% number = rand(1..7) %>
<div class="item num<%= number.to_s %>" id="<%= @id %>">
          <h4><%= @name %></h4>
          <br>
          <h5><%= @description %></h5>
          <%= form_tag("/users/#{@user_id}/rooms/#{@id}", method: :delete, remote: true) do %>
            <div class="delete"><%= submit_tag 'Delete' %>
            </div>
          <% end %>
</div>
